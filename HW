print("Пароль принят")
else: print("Пароль не принят")


print('введите число')
A = int(input())
print('введите второе число')
B = int(input())
if A >= B:
print(str(A),'- наибольшее число')
print(str(B), " - наименьшее число")
else:
print(str(B), '- наибольшее число')
print(str(A), " - наименьшее число")


print('введите возраст')
a = int(input())
if a >= 18:
print('доступ разрещён')
else:
print('доступ запрещён')

print('введите число')
d = int(input())
a = str(d % 10)
b = str(d//10 % 10)
c = str(d//100 % 10)
if a > b and a > c:
print(a)
elif b > a and b > c:
print(b)
else:
print(c)

print("введите число")
a = int(input())
b = int(input())
c = int(input())
d = int(input())
count = 0
g = 0
if a % 2 > 0:
count += 1
g += a
if b % 2 > 0:
count += 1
g += b
if c % 2 > 0:
count += 1
g += c
if d % 2 > 0:
count += 1
g += d
print(count, 'количество ничетных чисел')
print(g, "сумма ничётных чисел")








