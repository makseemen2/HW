print('задание 2')
s = 0
print('Введите число от 1 до 7')
n = int(input())
while n > 7:
print('Не верно')
n = int(input())
for b in range(n, 8):
s += (2*n+1)/4*n
print(s/5)

print('задание 3')
n = int(input())
a = n//2
b = a
for y in range(1, a+1):
print('1 ' * y + '0 ' * (n - y * 2) + '1 ' * y)
if n % 2 != 0:
print('1 ' * n)
else: b -= 1
for y in range(b, 0, -1):
print('1 ' * y + '0 ' * (n - y * 2) + '1 ' * y)
s = (n*3)-6
print(f'Сумма трех последних столбцов равна:', s)






дз 6





print('программа 1')
a = int(input('Задайте размер списка:'))
b = int(input('Задайте начало диапозона:'))
c = int(input('Задайте конец диапозона:'))
n = list()
for i in range(1, a+1):
q = randint(b, c)
n.append(q)
print(n)
print(len(n))
print(max(n))
print(min(n))
w = sorted(n)
print(w)
print(w[::-1])

print('программа 2')
a = int(input())
n = list()
for i in range(1, a+1):
q = randint(0, 1)
n.append(q)
print(n)
print(n.count(0))
print(n.count(1))
s = list()
for j in range(len(n)):
if n[j] == 0:
s.append(j)
print(s)

print('программа 3')
a = int(input('введите размер списка '))
n = list()
for i in range(1, a+1):
q = randint(0, 9)
n.append(q)
print(n)
b = int(input('введите число которое нужно удалить '))
for j in range(len(n)-1, -1, -1):
if n[j] == b:
n.pop(j)
print(n)

 
 
 
 
 
 
