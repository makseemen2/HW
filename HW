print('программа 3')
n = input()
s = 1
if n.isdigit():
for i in n:
print(int(i) * 10)
elif n.find("q") == 1:
print(n.count("q"))

else:
r = 0
for i in n:
if i == '!' or i == '?' or i == '#' or i == '/' or i == '*' or i == '&':
print(r)
r += 1

print('программа 4')
while True:
s = input("Задайте строку: ")
if s.upper() == "STOP":
break
if s.isalpha():
print("Буквенная строка")
elif s.isdigit():
print("Цифровая строка")
elif s.isalnum():
print("Смешанная строка")
